{
  "name": "woven-design-collective",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "start": "ng serve --project=interior-design -o",
    "build": "ng build interior-design --prod --output-path docs",
    "compress-images": "node compressImages.js",
    "postbuild": "echo www.wovendesigncollaborative.com > docs/CNAME && cp docs/index.html docs/404.html",
    "test": "ng test",
    "lint": "tslint --project tsconfig.json",
    "run-server": "http-server -p 8080 -c-1 docs/",
    "analyse-bundle": "yarn run build --stats-json && webpack-bundle-analyzer docs/stats-es2015.json",
    "e2e": "ng e2e",
    "deploy": "ng deploy interior-design --cname=woven-interiors.com"
  },
  "hooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.0-next.5",
    "@angular/cdk": "^11.2.5",
    "@angular/common": "~12.0.0-next.5",
    "@angular/compiler": "~12.0.0-next.5",
    "@angular/core": "~11.2.6",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "~12.0.0-next.5",
    "@angular/material": "^11.2.5",
    "@angular/platform-browser": "~12.0.0-next.5",
    "@angular/platform-browser-dynamic": "~12.0.0-next.5",
    "@angular/router": "~12.0.0-next.5",
    "@angular/service-worker": "~12.0.0-next.5",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "global": "^4.4.0",
    "ng-animate": "^0.3.4",
    "ng2-fittext": "^1.2.6",
    "ngx-teximate": "^0.3.0",
    "rxjs": "~6.6.6",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.5",
    "@angular/cli": "~11.2.5",
    "@angular/compiler-cli": "~12.0.0-next.5",
    "@angular/language-service": "~12.0.0-next.5",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^6.0.0",
    "http-server": "^0.12.1",
    "husky": "^4.0.9",
    "imagemin": "^7.0.1",
    "imagemin-keep-folder": "^5.3.2",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-webp": "^5.1.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^9.5.0",
    "prettier": "1.19.1",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "tslint-angular": "^3.0.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-etc": "^1.10.0",
    "tslint-plugin-prettier": "^2.1.0",
    "typescript": "^4.2.3",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}