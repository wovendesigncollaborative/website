          "options": {
            "protractorConfig": "projects/integrated-design/e2e/protractor.conf.js",
            "devServerTarget": "integrated-design:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "integrated-design:serve:production"
            }
          }
        }
      }
    },
